.content {
  margin-left: 0.7em;
  margin-top: 20rpx;
}

textarea {
  border: 1px solid #ccc;
  color: #000;
  width: 95%;
  padding-left: 5px;
  margin-top: 24rpx;
}

.con {
  width: 100%;
  height: auto;
  margin-top: 10rpx;
  display: block;
}

.contact {
  display: block;
  padding-left: 0.7rem;
  color: #333;
  line-height: 3rem;
}

.contact input {
  border: 1px solid #ccc;
  margin-left: 92px;
  margin-top: -49px;
  width: 70%;
  margin-bottom: 10px;
  padding-left: 5px;
  height: 2.4em;
}

.btn-area {
  background-color: #318000;
  color: #fff;
}

.home {
  position: fixed;
  top: 8rpx;
  right: 13rpx;
  z-index: 4;
  width: 70rpx;
  height: 70rpx;
  display: block;
}

.wrap{
  display:flex;
  width:100%;
  flex:wrap;
  border-bottom:6rpx solid #eee;
  padding-top:20rpx;
  padding-bottom:20rpx;
}

.wrap navigator{
  width:calc(30% - 4px);
  margin-left:2.5%;
  border:4rpx solid #35495e;
  color:#35495e;
  text-align:center;
  font-size:36rpx;
  line-height:70rpx;
}

.wrap view{
  height:40rpx;
  width:40rpx;
  display:inline-block;
  background-color:#093;
  color:#fff;
  text-align:center;
  line-height:40rpx;
  font-weight:900;
  font-family:微软雅黑;
  font-size:32rpx;
  border-radius:50%;
}


