/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-family: '微软雅黑';
  font-weight: 200;
}

.header {
  margin-top: 10rpx;
  padding-bottom: 10rpx;
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  /* margin-bottom:20rpx; */
  border-bottom: 6rpx solid #eee;
}

.template-1 {
  width: 16%;
  /* float: left; */
  margin-right: 2%;
  margin-left: 2%;
}

.template-1 button {
  background-color: #00bab8;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  font-weight: 600;
  font-size: 32rpx;
  padding-left: 0;
  padding-right: 0;
}

.copy {
  height: 100rpx;
  line-height: 100rpx;
  color: #999;
  text-align: center;
  font-size: 24rpx;
}

page {
  font-family: 微软雅黑;
}

.zs-r {
  float: right;
  margin-right: 20rpx;
}

.fz-l {
  display: inline-block;
  line-height: 50rpx;
}

.zs {
  margin-left: 4%;
  margin-top: 20rpx;
}

.b-img {
  border-radius: 50%;
  width: 50rpx;
  height: 50rpx;
  position: relative;
  top: 10rpx;
  margin-right: 10rpx;
}

.ben {
  float: left;
  width: 50rpx;
  height: 50rpx;
  background-color: #093;
  border-radius: 50%;
  color: #fff;
  line-height: 50rpx;
  text-align: center;
  margin-right: 10rpx;
}

.fz {
  padding-left: 4%;
  margin-top: 30rpx;
  border-top: 4rpx solid #ccc;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #ccc;
}

.f-x {
  width: 40rpx;
  height: 40rpx;
  position: relative;
  top: 12rpx;
  margin-right: 10rpx;
  border-radius: 50%;
}

.shuoming {
  color: #999;
  font-size: 24rpx;
  text-align: center;
  margin-bottom: 40rpx;
}
